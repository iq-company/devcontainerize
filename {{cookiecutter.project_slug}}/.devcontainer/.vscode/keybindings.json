// Place your key bindings in this file to override the defaults
[
  {
      "key": "shift+alt+r",
      "command": "workbench.action.debug.start",
      "when": "config.iq.keybindings.enabled && !inDebugMode"
  },
  {
      "key": "f5",
      "command": "workbench.action.debug.stepInto",
      "when": "config.iq.keybindings.enabled && inDebugMode"
  },
  {
      "key": "f6",
      "command": "workbench.action.debug.stepOver",
      "when": "config.iq.keybindings.enabled && inDebugMode"
  },
  {
      "key": "f7",
      "command": "workbench.action.debug.stepOut",
      "when": "config.iq.keybindings.enabled && inDebugMode"
  },
  {
      "key": "f8",
      "command": "workbench.action.debug.continue",
      "when": "config.iq.keybindings.enabled && inDebugMode"
  },
  {
      "key": "f8",
      "command": "workbench.action.tasks.runTask",
      "args": "Restart Bench",
      "when": "config.iq.keybindings.enabled && !inDebugMode"
  },
  {
      "key": "f10",
      "command": "workbench.action.tasks.runTask",
      "when": "config.iq.keybindings.enabled"
  },
  {
      "key": "shift+alt+d",
      "command": "editor.action.duplicateSelection",
      "when": "config.iq.keybindings.enabled"
  },
  {
    "key": "shift+f12",
    "command": "debug.jumpToCursor",
    "when": "config.iq.keybindings.enabled && editorTextFocus && inDebugMode"
  },
  {
    "key": "shift+l",
    "command": "workbench.action.nextEditorInGroup",
    "when": "config.iq.keybindings.enabled && editorTextFocus && neovim.mode == 'normal'"
  },
  {
    "key": "shift+h",
    "command": "workbench.action.previousEditorInGroup",
    "when": "config.iq.keybindings.enabled && editorTextFocus && neovim.mode == 'normal'"
  },
  {
    "key": "shift+h",
    "command": "workbench.action.previousPanelView",
    "when": "config.iq.keybindings.enabled && panelFocus"
  },
  {
    "key": "shift+l",
    "command": "workbench.action.nextPanelView",
    "when": "config.iq.keybindings.enabled && panelFocus"
  },
  {
    "key": "f12",
    "command": "-editor.action.goToDeclaration",
    "when": "config.iq.keybindings.enabled && editorHasDefinitionProvider && editorTextFocus"
  },
  {
    "key": "f12",
    "command": "remote-containers.reopenLocally",
    "when": "config.iq.keybindings.enabled && remoteName"
  },
  {
    "key": "ctrl+f12",
    "command": "remote-containers.rebuildContainer",
    "when": "config.iq.keybindings.enabled && remoteName"
  },
  {
    "key": "f12",
    "command": "remote-containers.reopenInContainer",
    "when": "config.iq.keybindings.enabled && !remoteName"
  }
]
